{
  "name": "Test",
  "description" : "Extension test",
  "manifest_version": 2,
  "version": "0.0.1",
   "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": [
    "bookmarks",
    "webNavigation"
  ],
  "browser_action": {
    "default_icon":"icons/ico.png"
  }
}